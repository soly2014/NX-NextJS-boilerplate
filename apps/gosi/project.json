{
  "name": "gosi",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/gosi",
  "projectType": "application",
  "tags": ["scope:gosi"],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/gosi",
        "root": "apps/gosi",
        "fileReplacements": [],
        "nextConfig": "apps/gosi/next.config.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": []
        }
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "gosi:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "gosi:build:production",
          "dev": false
        }
      }
    }
  }
}
